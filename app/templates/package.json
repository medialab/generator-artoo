{
  "name": "<%= _.slugify(bookmarkName) %>",
  "version": "0.0.1",
  "description": "<%= bookmarkDescription %>",
  "scripts": {
    "start": "gulp work"
  },
  "main": "<%= bookmarkName %>.js",
  "dependencies": {
    "gulp": "~3.8.6",
    "gulp-artoo": "*",
